pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
                echo 'cloning the repository'
            }
        }
        stage('Build') {
            steps {
                echo 'Build the code'
            }
        }
        stage('test') {
            steps {
                echo 'test the package'
            }
        }
        stage('deploy') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
